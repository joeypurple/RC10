import urllib.request

#Class to pulldown and save HTML as text file
class Pulldown:
    #Constructor to save hardcoded URL
    def __init__(self):
        self.url_file, headers = urllib.request.urlretrieve\
            ('https://www.teamassociated.com/cars_and_trucks/RC10B6.1/Team/manuals_setups/')
    #Compare saved URL data to current URL data
    def compare_file(self):
        saved_url = open(self.url_file, "r")
        saved_url_print = saved_url.readlines()
        saved_url_file = open('C://Users//Joe//Desktop//blank.txt', "r+")
        saved_url_file.write(str(saved_url_print))
        if saved_url_file == saved_url_file:
             print("A is equal to B")
        else :
             print ("A is not equal to B")
        #print (a.readlines(500))
        #print (b.readlines(500))

#Run the functions
if __name__ == "__main__":
    main = Pulldown()
    main.compare_file()
